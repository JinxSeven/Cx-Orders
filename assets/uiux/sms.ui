      <property name="styleSheet">
       <string notr="true">/* QTableWidget Style */
QTableWidget {
    border-radius: 10px;
    gridline-color: #E0E0E0; /* Light grid lines */
    color: white; /* Dark text color */
    font: 10pt &quot;Poppins&quot;;
    background-color: #292D3E;
    border: 2px solid #292D3E; /* Border color same as background to enhance rounded effect */
}

/* QTableWidget Item Style */
QTableWidget::item {
    border: none; /* Ensure no extra border on items */
}

/* Header Style */
QHeaderView::section {
    background-color: #F5F5F5; /* Light background */
    padding: 5px;
    border: none;
    border-bottom: 1px solid #E0E0E0;
    color: black;
    font: 10pt &quot;Poppins&quot;;
}

/* Vertical Scrollbar */
QScrollBar:vertical {
    width: 0px; /* Hides the scrollbar track */
    background: none; /* Optional: removes background color for better invisibility */
}

/* Horizontal Scrollbar (optional) */
QScrollBar:horizontal {
    height: 0px; /* Hides the scrollbar track */
    background: none; /* Optional: removes background color for better invisibility */
}

/* QTableWidget Viewport Style */
QTableView::viewport {
    border-radius: 10px;
    background-color: #292D3E; /* Ensures viewport has same background */
}

/* Ensuring corners stay rounded by overlapping header and viewport */
QTableView::corner {
    background-color: #292D3E; /* Same as the QTableWidget background color */
    border-radius: 10px 0 0 0; /* Top-left rounded corner */
}</string>
